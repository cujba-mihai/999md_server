{
  "name": "999-md-clone",
  "version": "0.0.1",
  "description": "",
  "author": "Mihai Cujba, Daniel Raileanu",
  "license": "UNLICENSED",
  "scripts": {
    "heroku": "heroku logs --tail -a tripple-nine-server",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "create:seeder": "node ./scripts/create-seeder",
    "unseed:seeder": "node ./scripts/module/unseed-seeder",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export DOTENV_CONFIG_PATH=.env.production && nest start",
    "dev": "export DOTENV_CONFIG_PATH=.env.development && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "apollo-server-express": "^3.11.1",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "graphql": "^16.6.0",
    "graphql-fields-list": "^2.2.4",
    "graphql-tools": "^8.3.12",
    "mongoose": "^6.7.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "pretty-quick": "^3.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.6.0",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/jest": "29.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.10",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "husky": "^8.0.2",
    "jest": "29.4.3",
    "prettier": "^2.8.0",
    "rimraf": "^4.0.0",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.2",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "volta": {
    "node": "18.13.0",
    "npm": "9.5.1"
  },
  "engines": {
    "node": "18.13.0",
    "npm": "9.5.1"
  }
}
